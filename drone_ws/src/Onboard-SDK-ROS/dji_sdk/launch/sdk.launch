<launch>
    <node pkg="dji_sdk" type="dji_sdk_node" name="dji_sdk" output="screen">
    <!-- node parameters -->
    <param name="serial_name" type="string" value="/dev/ttyTHS1"/>
    <param name="baud_rate" type="int" value="230400"/>
    <param name="app_id" type="int" value="1052251"/>
    <param name="app_version" type="int" value="1"/>
    <param name="align_time" type="bool" value="false"/>
    <param name="enc_key" type="string" value="e64dbbee5e36ce505e4cb44c51458327eae4bccb21692a1fdd383e8c0399d30c"/>
    <param name="use_broadcast" type="bool" value="false"/>
    </node>
    <!-- Added by Michael, TODO: add delay  -->
    <node name="simu_to_dji" pkg="gst_track" type="simu_to_dji" />
    <!-- node name="dji_mode_detector" pkg="gst_track" type="dji_mode_detector" / -->
</launch>
